<?xml version="1.0"?>

<config>
    <tabs>
        <magestore translate="label">
            <label>Magestore Extension</label>
            <sort_order>400</sort_order>
        </magestore>
    </tabs>
    <sections>
        <deliverytime translate="label" module="deliverytime">
            <class>separator-top</class>
            <label>Delivery Time</label>
            <tab>magestore</tab>
            <frontend_type>text</frontend_type>
            <sort_order>300</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>				<general translate="label">                    <label>General Configuration</label>                    <frontend_type>text</frontend_type>                    <sort_order>1</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>0</show_in_store>                    <fields>						<enable translate="label">                            <label>Enable</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>1</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </enable>												<time_interval translate="label">                             <label>Time Interval</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>2</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </time_interval>												<minimum_gap translate="label">                             <label>Minimum Time Gap</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>3</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </minimum_gap>												<ship_day translate="label">                             <label>Maximum Day To Ship</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>3</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </ship_day>												<message translate="label">                             <label>Message</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>4</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </message>																	</fields>					</general>							<time_schedule translate="label">                    <label>Time Schedule</label>                    <frontend_type>text</frontend_type>                    <sort_order>2</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>0</show_in_store>                    <fields>						<monday_open translate="label">                            <label>Monday Open</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>1</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </monday_open>												<monday_close translate="label">                            <label>Monday Close</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>2</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </monday_close>												<tuesday_open translate="label">                            <label>Tuesday Open</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>3</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </tuesday_open>												<tuesday_close translate="label">                            <label>Tuesday Close</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>4</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </tuesday_close>																		<wednesday_open translate="label">                            <label>Wednesday Open</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>5</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </wednesday_open>												<wednesday_close translate="label">                            <label>Wednesday Close</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>6</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </wednesday_close>												<thursday_open translate="label">                            <label>Thursday Open</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>7</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </thursday_open>												<thursday_close translate="label">                            <label>Thursday Close</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>8</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </thursday_close>												<friday_open translate="label">                            <label>Friday Open</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>9</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </friday_open>												<friday_close translate="label">                            <label>Friday Close</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>10</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </friday_close>												<saturday_open translate="label">                            <label>Saturday Open</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>11</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </saturday_open>												<saturday_close translate="label">                            <label>Saturday Close</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>12</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </saturday_close>												<sunday_open translate="label">                            <label>Sunday Open</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>13</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </sunday_open>												<sunday_close translate="label">                            <label>Sunday Close</label>                            <frontend_type>text</frontend_type>                                                        <sort_order>14</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </sunday_close>					</fields> 				</time_schedule>		
            </groups>
        </deliverytime>        
    </sections>
</config>